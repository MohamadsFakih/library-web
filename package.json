{
  "name": "library-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run db:seed && npx next dev",
    "build": "node scripts/ensure-postgres-for-build.js && prisma generate && prisma db push && npm run db:seed && npx next build",
    "start": "npm run db:seed && npx next start",
    "lint": "eslint .",
    "postinstall": "node scripts/ensure-postgres-for-build.js && prisma generate",
    "db:push": "prisma db push",
    "db:seed": "node prisma/seed.cjs",
    "db:migrate": "prisma migrate deploy"
  },
  "prisma": {
    "seed": "node prisma/seed.cjs"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@vercel/blob": "^0.25.0",
    "bcryptjs": "^2.4.3",
    "next": "15.0.3",
    "next-auth": "^5.0.0-beta.25",
    "openai": "^4.73.0",
    "prisma": "^5.22.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^9",
    "eslint-config-next": "15.0.3",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5"
  }
}
